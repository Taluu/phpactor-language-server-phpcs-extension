{
    "name": "taluu/phpactor-language-server-phpcs-extension",
    "description": "PHPActor language server extension for phpcs diagnostics",
    "type": "phpactor-extension",
    "require": {
        "php": "7.4.* || 8.0.*",
        "phpactor/container": "^1.3",
        "phpactor/language-server-extension": "^0.4.0",
        "amphp/process": "^1.1",
        "phpactor/language-server-protocol": "^0.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Clavi√©",
            "email": "clavie.b@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\LanguageServerPhpCs\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
